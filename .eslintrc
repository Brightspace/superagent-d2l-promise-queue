{
    "env": {
        "browser": true,
        "node": true
    },
    "rules": {
        "semi": 2,
        "new-cap": 0,
        "quotes": 0,
        "no-trailing-spaces": 0,
        "curly": 0,
        "no-unused-vars": 0,
        "no-underscore-dangle": 0,
        "eol-last": 0,
        "key-spacing": 0,
        "no-shadow": 0,
        "comma-spacing": 0,
        "space-infix-ops": 0,
        "strict": 0,
        "no-multi-spaces": 0,
        "no-mixed-spaces": 0,
        "no-spaced-func": 0,
        "no-unused-expressions": 0,
        "no-mixed-spaces-and-tabs": 0
    }
}